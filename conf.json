{
	"source": {
		"include": ["./assets/scripts", "./jest_tests"],
		"includePattern": ".js",
		"exclude": ["(node_modules)"],
		"excludePattern": "(^|\\/|\\\\)_|.json"
	},
	"sourceType": "module",
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc","closure"]
	},
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false,
		"includeDate": false
	},
	"opts": {
		"destination": "./specs/documentation/generated/",
		"recurse": true
	}
}